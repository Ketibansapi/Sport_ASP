@model IEnumerable<SportTransfer4.Models.Transfer>

@{
    ViewBag.Title = "Transfers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Transfers</h2>
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Transfer</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var transfer in Model)
        {
            <tr>
                <td>@transfer.Name</td>
            </tr>
        }
    </tbody>
</table>